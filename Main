local placeId = game.PlaceId

-- Lấy CoreGui đúng cho executor
local core = (gethui and gethui()) or game:GetService("CoreGui")

-- GUI container
local screenGui = Instance.new("ScreenGui")
screenGui.IgnoreGuiInset = true
screenGui.ResetOnSpawn = false
screenGui.Parent = core

-- Frame (UI chính)
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0.25, 0, 0.25, 0)
frame.Position = UDim2.new(1, 0, 0, 0)
frame.AnchorPoint = Vector2.new(1, 0)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.BackgroundTransparency = 1
frame.Parent = screenGui

-- Bo góc
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 15)
corner.Parent = frame

-- Gradient
local gradient = Instance.new("UIGradient")
gradient.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(50, 50, 50)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(25, 25, 25))
})
gradient.Rotation = 90
gradient.Parent = frame

-- Shadow UI
local shadow = Instance.new("ImageLabel")
shadow.Parent = frame
shadow.BackgroundTransparency = 1
shadow.Size = UDim2.new(1, 30, 1, 30)
shadow.Position = UDim2.new(0.5, 0, 0.5, 0)
shadow.AnchorPoint = Vector2.new(0.5, 0.5)
shadow.Image = "rbxassetid://1316045217"
shadow.ImageTransparency = 0.4

-- Icon
local icon = Instance.new("ImageLabel")
icon.Parent = frame
icon.Size = UDim2.new(0.3, 0, 0.3, 0)
icon.Position = UDim2.new(0.05, 0, 0.1, 0)
icon.BackgroundTransparency = 1
icon.Image = "rbxassetid://107169193664829"

-- Text
local textLabel = Instance.new("TextLabel")
textLabel.Size = UDim2.new(0.6,0,0.6,0)
textLabel.Position = UDim2.new(0.38,0,0.25,0)
textLabel.BackgroundTransparency = 1
textLabel.TextColor3 = Color3.fromRGB(255,255,255)
textLabel.TextScaled = true
textLabel.TextXAlignment = Enum.TextXAlignment.Left
textLabel.Font = Enum.Font.GothamBold
textLabel.TextTransparency = 1
textLabel.Parent = frame

-- Fade function
local function fade(obj, from, to, time)
    local TweenService = game:GetService("TweenService")
    local tween = TweenService:Create(obj, TweenInfo.new(time, Enum.EasingStyle.Quad), {BackgroundTransparency = to})
    tween:Play()
end

local function fadeText(obj, from, to, time)
    local TweenService = game:GetService("TweenService")
    local tween = TweenService:Create(obj, TweenInfo.new(time, Enum.EasingStyle.Quad), {TextTransparency = to})
    tween:Play()
end

local function fadeImage(obj, from, to, time)
    local TweenService = game:GetService("TweenService")
    local tween = TweenService:Create(obj, TweenInfo.new(time, Enum.EasingStyle.Quad), {ImageTransparency = to})
    tween:Play()
end

-- Slide In
local function slideIn()
    frame:TweenPosition(
        UDim2.new(0.75, 0, 0, 0),
        Enum.EasingDirection.Out,
        Enum.EasingStyle.Quad,
        0.4,
        true
    )
    fade(frame, 1, 0.1, 0.4)
    fadeImage(icon, 1, 0, 0.4)
    fadeImage(shadow, 1, 0.4, 0.4)
    fadeText(textLabel, 1, 0, 0.4)
end

-- Slide Out
local function slideOut()
    fade(frame, 0.1, 1, 0.4)
    fadeImage(icon, 0, 1, 0.4)
    fadeImage(shadow, 0.4, 1, 0.4)
    fadeText(textLabel, 0, 1, 0.4)

    task.wait(0.1)

    frame:TweenPosition(
        UDim2.new(1, 0, 0, 0),
        Enum.EasingDirection.In,
        Enum.EasingStyle.Quad,
        0.4,
        true
    )
end

if placeId == 18687417158 then
    textLabel.Text = "SeaMinhSaken"
    slideIn()
    task.wait(4)
    slideOut()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Sea-MinhHub/SeaMinhHub/main/SeaMinhSaken", true))()

elseif placeId == 14044547200 then
    textLabel.Text = "Sea | Minh Hub Untitled Tag Game V 1.0"
    slideIn()
    task.wait(4)
    slideOut()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Sea-MinhHub/SeaMinhHub/main/untitled%20tag%20game", true))()

else
    textLabel.Text = "Sea | Minh Hub does not support this game!!"
    slideIn()
    task.wait(4)
    slideOut()
end
